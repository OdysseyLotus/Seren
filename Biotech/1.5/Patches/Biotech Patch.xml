	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ScenarioDef[defName="SerenRelocated"]/scenario/parts/li[1]</xpath>
				<value>
					<li Class="ScenPart_ConfigPage_ConfigureStartingPawns_Xenotypes">
						<def>ConfigurePawnsXenotypes</def>
						<pawnChoiceCount>8</pawnChoiceCount>
						<overrideKinds>
							<li>
								<xenotype>Seren_Base</xenotype>
								<pawnKind>SerenColonist</pawnKind>
							</li>
						</overrideKinds>
						<xenotypeCounts>
							<li>
								<xenotype>Seren_Base</xenotype>
								<count>4</count>
								<description>seren colonists</description>
								<requiredAtStart>true</requiredAtStart>
								<allowedDevelopmentalStages>Adult, Child</allowedDevelopmentalStages>
							</li>
						</xenotypeCounts>
					</li>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ScenarioDef[defName="SerenPrisonCrash"]/scenario/parts/li[1]</xpath>
				<value>
					<li Class="ScenPart_ConfigPage_ConfigureStartingPawns_Xenotypes">
